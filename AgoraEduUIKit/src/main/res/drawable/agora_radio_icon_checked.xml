<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:paddingMode="nest">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/fcr_white"/>
            <stroke android:color="@color/theme_blue_light" android:width="@dimen/stroke_small"/>
            <size android:width="@dimen/agora_kick_dialog_option_button_size"
                android:height="@dimen/agora_kick_dialog_option_button_size"/>
        </shape>
    </item>
    <item android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="@color/theme_blue_light"/>
            <size android:width="@dimen/agora_kick_dialog_option_button_size"
                android:height="@dimen/agora_kick_dialog_option_button_size"/>
        </shape>
    </item>
</layer-list>